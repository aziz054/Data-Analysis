    "df.dtypes #the numerical values 'bore' and 'stroke' describe the engines, \n",
    "#so you should expect them to be of the type 'float' or 'int'; however, they are shown as type 'object'."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 365,
   "id": "a6bd64d3",
   "metadata": {},
   "outputs": [],
   "source": [
    "df[[\"bore\",\"stroke\"]] = df[[\"bore\",\"stroke\"]].astype('float')\n",
    "df[[\"normalized-losses\"]] = df[[\"normalized-losses\"]].astype(\"int\")\n",
    "df[[\"price\"]] = df[[\"price\"]].astype(\"float\")\n",
    "df[[\"peak-rpm\"]] = df[[\"peak-rpm\"]].astype(\"float\")"
